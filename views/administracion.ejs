 <main class="contenedor  panel-administrativo">
    <h1><%- nombrePagina %>
    <div class="contenedor-botones">
        <a href="/nuevo-grupo" class="btn btn-amarillo">Nuevo Grupo</a>
        <a href="/nuevo-meeti" class="btn btn-azul">Nuevo Metti</a>
        <a href="/editar-perfil" class="btn btn-rosa">Edictar Perfil</a>
        <a href="/imagem-grupo" class="btn btn-amarillo">Imagen Perfil</a>
    </div>
    <div class="seccion-admin">
        <h2>Tus MeetiÂ´s</h2>
        <ul><li>
            <div class="informacion-admin">
                <p class="fecha">Lunes, 22 de Mayo de 2019</p>
                <h3>E-commerce en Venezuela</h3>
                <small> 23 asistente</small>
            </div>
            <div class="acciones contenerdor-botones">
                <a href="#" class="btn btn-verde">Edictar</a>
                <a href="#" class="btn btn-azul2">Asistentes</a>
                <a href="#" class="btn btn-rojo">Eliminar</a>
                
            </div>
        </li></ul>
        <div class="seccion-admin">
        <h2>Tus Grupos</h2>
            <% if(grupos.length) {%>    
                <ul>
                    <% grupos.forEach(grupo => {%>
                        <li>
                            <div class="informacion-admin">
                                <h3><%= grupo.nombre %></h3>                          
                            </div>
                            <div class="acciones contenerdor-botones">
                                <a href="/editar-grupo/<%= grupo.id%>" class="btn btn-verde">Edictar</a>
                                <a href="/imagen-grupo/<%= grupo.id%>" class="btn btn-azul2">Imagen</a>
                                <a href="/eliminar-grupo/<%= grupo.id%>" class="btn btn-rojo">Eliminar</a>
                            </div>
                        </li>
                    <%}); %>
                </ul>                  
            <%} else{%>
                <p>No tienes  grupo</p>
            <%} %>
        </div>
    </div>
</main>